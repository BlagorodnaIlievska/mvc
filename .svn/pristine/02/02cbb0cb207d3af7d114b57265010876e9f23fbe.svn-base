.notacordion h3 {
    line-height: 1.2 !important;
    color: #FFF !important;
    border-radius: 0 !important;
    text-decoration: none !important;
    text-shadow: 0px 1px 0px rgba(0, 0, 0, .35) !important;
    background: #6c6e74 !important;
    padding: 5px 10px 5px 5px !important;
    background: -moz-linear-gradient(top, #6c6e74 0%, #4b4d51 100%) !important;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #6c6e74), color-stop(100%, #4b4d51)) !important;
    background: -webkit-linear-gradient(top, #6c6e74 0%, #4b4d51 100%) !important;
    background: -o-linear-gradient(top, #6c6e74 0%, #4b4d51 100%) !important;
    background: -ms-linear-gradient(top, #6c6e74 0%, #4b4d51 100%) !important;
    background: linear-gradient(top, #6c6e74 0%, #4b4d51 100%) !important;
}
.notacordion h3.ui-accordion-header-active.ui-state-active {
    border-radius: 0 !important;
    background: -moz-linear-gradient(center top , #3F3F3F 7%, #303030 100%) repeat scroll 0 0 transparent !important;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(7%,#3F3F3F), color-stop(100%, #303030)) !important;
    font-size: 13px !important;
    border: none !important;
    color: #ADD8E6 !important;
}
.notacordion h3 span {float: left !important;margin: 2px 0 0 2px !important;background: url("../images/plus.png") no-repeat scroll 0 0 transparent !important;}
.notacordion h3.ui-accordion-header-active.ui-state-active span {background: url("../images/minus.png") no-repeat scroll 0 0 transparent !important;}
.notacordion h3 a {margin: 10px !important;color: #FDFDFD !important;}
.notacordion h3.ui-accordion-header-active.ui-state-active a {color: #ADD8E6 !important;}

