            </tr>
            </thead>
            <tbody>
            <tr class='hiderow trow' id="">
                <td class="deletebaranje"><img width="15" height="15" alt="" src="/images/delete.png"></td>
                <td class="editbaranje"><img width="15" height="15" alt=""
                                             src="/images/edit.png"></td>

                <td class="brojb"></td>
                <td class="datumb"></td>
                <td class="baranjem"></td>
                <td class="baranjep"></td>
                <td class="kl"></td>


            </tr>
            </tbody>
        </table>
    </div>

</div>
<script>
    jQuery(document).ready(function () {
        baranja_napolnilista();

        jQuery('.bnovobaranje').click(function () {
            novo_baranje();
        });
        jQuery('.tbaranja .deletebaranje').live('click',function () {
            //console.log(jQuery(this).parent().attr('dogovorid'));
            delete_baranje(jQuery(this).parent().attr('id'))
        });
        jQuery('.tbaranja .editbaranje').live('click',function () {
            //console.log(jQuery(this).parent().attr('dogovorid'));
            console.log(jQuery(this).parent().attr('id')) ;
            izmeni_baranje(jQuery(this).parent().attr('id'));
        } );
    });
</script>
