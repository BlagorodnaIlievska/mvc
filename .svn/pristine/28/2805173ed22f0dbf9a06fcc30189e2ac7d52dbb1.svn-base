                    $this->db->where('detID', $detalid);
                    $this->db->delete('det');
                } else {
                    $artikl_info = $this->getArtiklInfoById($detalid);
                    $prev_sostojba = $artikl_info->sostojba;
                    $prev_kolicina = $artikl_info->kolicina;
