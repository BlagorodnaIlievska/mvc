.bojan-dropdownmenu {
    margin: 0;
    /*padding: 7px 6px 0;*/
    /*background: #7d7d7d url(img/gradient.png) repeat-x 0 -110px;*/
    line-height: 100%;

    border-radius: 2em;
    -webkit-border-radius: 2em;
    -moz-border-radius: 2em;

    -webkit-box-shadow: 0 1px 3px rgba(0,0,0, .4);
    -moz-box-shadow: 0 1px 3px rgba(0,0,0, .4);
}
.bojan-dropdownmenu li {

    padding: 0 0 8px;
    float: left;
    position: relative;
    list-style: none;
}


/* main level link */
.bojan-dropdownmenu a {
    font-weight: bold;
    color: #e7e5e5;
    text-decoration: none;
    display: block;
    padding:  8px 20px;
    margin: 0;

    -webkit-border-radius: 1.6em;
    -moz-border-radius: 1.6em;

    text-shadow: 0 1px 1px rgba(0,0,0, .3);
}
.bojan-dropdownmenu a:hover {
    background: #000;
    color: #fff;
}

/* main level link hover */
.bojan-dropdownmenu .current a, .bojan-dropdownmenu li:hover > a {
    background: #666 url(img/gradient.png) repeat-x 0 -40px;
    color: #444;
    border-top: solid 1px #f8f8f8;

    -webkit-box-shadow: 0 1px 1px rgba(0,0,0, .2);
    -moz-box-shadow: 0 1px 1px rgba(0,0,0, .2);
    box-shadow: 0 1px 1px rgba(0,0,0, .2);

    text-shadow: 0 1px 0 rgba(255,255,255, 1);
}

/* sub levels link hover */
.bojan-dropdownmenu ul li:hover a, .bojan-dropdownmenu li:hover li a {
    background: none;
    border: none;
    color: #666;

    -webkit-box-shadow: none;
    -moz-box-shadow: none;
}
.bojan-dropdownmenu ul a:hover {
    background: #0078ff url(img/gradient.png) repeat-x 0 -100px !important;
    color: #fff !important;

    -webkit-border-radius: 0;
    -moz-border-radius: 0;

    text-shadow: 0 1px 1px rgba(0,0,0, .1);
}

/* dropdown */
.bojan-dropdownmenu li:hover > ul {
    display: block;
}

/* level 2 list */
.bojan-dropdownmenu ul {
    display: none;

    z-index: 9999999999999;
    margin: 0;
    padding: 0;
    width: 250px;
    position: absolute;
    top: 35px;
    left: 0;
    /*background: #ddd url(img/gradient.png) repeat-x 0 0;*/
    background-color: #B9C9FE;
    border: solid 1px #b4b4b4;

    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;

    -webkit-box-shadow: 0 1px 3px rgba(0,0,0, .3);
    -moz-box-shadow: 0 1px 3px rgba(0,0,0, .3);
    box-shadow: 0 1px 3px rgba(0,0,0, .3);
}
.bojan-dropdownmenu ul li {
    float: none;
    margin: 0;
    padding: 0;
}

.bojan-dropdownmenu ul a {
    font-weight: normal;
    text-shadow: 0 1px 0 #fff;
}

/* level 3+ list */
.bojan-dropdownmenu ul ul {
    left: 181px;
    top: -3px;
}

/* rounded corners of first and last link */
.bojan-dropdownmenu ul li:first-child > a {
    -webkit-border-top-left-radius: 9px;
    -moz-border-radius-topleft: 9px;

    -webkit-border-top-right-radius: 9px;
    -moz-border-radius-topright: 9px;
}
.bojan-dropdownmenu ul li:last-child > a {
    -webkit-border-bottom-left-radius: 9px;
    -moz-border-radius-bottomleft: 9px;

    -webkit-border-bottom-right-radius: 9px;
    -moz-border-radius-bottomright: 9px;
}

/* clearfix */
.bojan-dropdownmenu:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
.bojan-dropdownmenu {
    display: inline-block;
}
.bojan-dropdownmenu ul li button {
    width: 100% !important;
    text-align: center;
}
.bojan-dropdownmenu ul li  {
    padding: 5px;
}
html[xmlns] .bojan-dropdownmenu {
    display: block;
}

* html .bojan-dropdownmenu {
    height: 1%;
}
