}

.filter-vrab label {font-size: 14px;line-height: 1.7;margin-bottom: 5px;white-space: nowrap;float: left;margin:0 5px;}
.filter-vrab input , .filter-vrab select , .filter-vrab span {float: left;}

.filter-vrab span {line-height: 1.6;}
.novvraboten-levomeni label  {line-height: 22px;width: 150px;font-size: 90%;white-space: nowrap; float: left;}
