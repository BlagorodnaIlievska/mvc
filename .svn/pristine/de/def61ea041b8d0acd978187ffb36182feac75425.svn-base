.novvraboten-levomeni input , .novvraboten-levomeni select {margin-bottom: 5px;width:240px; float: right; }
.novvraboten-levomeni select {width: 244px !important;}
.raboten-odnos-cr input {width: 110px;float: right;}
.pravenosnov-raboten-odnos-cr input , .brojfaksimil {width: 100%;margin-bottom: 5px;}
.raboten-odnos-cr {height: 25px;}
.rabotenodnos-info-cr label {
    float: left;
    width: 120px;
    line-height: 1.6;
}
.raboten-odnos-cr label {line-height: 1.6;}
.rabotenodnos-info-cr input , .rabotenodnos-info-cr select {margin-bottom: 5px;}
.novvraboten-podmeni fieldset .datumvrab {width: 90px;}
.vraboteni-glavna-lista .buttoncontainer {
    margin: 5px 0;
}
