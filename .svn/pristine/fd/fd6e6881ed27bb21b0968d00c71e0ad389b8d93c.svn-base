<script>

    jQuery(document).ready(function () {
        for (i = new Date().getFullYear(); i >= 1970; i--)
        {
            $('#yearpicker').append($('<option />').val(i).html(i));
        }
        jQuery('.prebaruvaj-comboboxes').live('change', function () {
            var polovina = jQuery('#prebaruvaj-po-godina option:selected').val();
            var godina = jQuery('#yearpicker option:selected').val();
            var formaction = "/index.php/statisticki_podatoci/statistika/prebaruvanje";
            var data = {
                polovina:polovina,
                godina:godina
            }
            jQuery.ajax({
                url:formaction,
                type:'POST',
                data:data,
                dataType:'json',
                success:function (response) {
                    var prenosi = response[0]['prenos'];
                    var prenosi_traum_ort = response[1]['prenos_traum'];
                    var prenosi_karil = response[2]['prenos_karil'];
                    var prenosi_uc = response[3]['prenos_uc'];

                    jQuery.each(prenosi, function (index, item) {
// console.log(index,item);
                        jQuery.each(item, function (index2, item2) {
                          // console.log(index2, item2);

                            jQuery("."+index2).val(item2);
                        });
                    });
                    jQuery.each(prenosi_karil, function(index3, item3){
                        //console.log(index3, item3);
                        jQuery.each(item3, function(index33, item33){
                            jQuery("."+index33).val(item33);
                            //console.log(index33, item33);
                        });
                    });
                    jQuery.each(prenosi_uc, function(index4, item4){
                        jQuery.each(item4, function(index44, item44){
                            jQuery("."+index44).val(item44);
                        });
                    });
                    for(var n = 1; n <= 7; n++){
var item = prenosi_traum_ort[n];
                        console.log(n, item);
                        jQuery.each(item, function(indexn, itemn){
                            console.log(indexn, itemn);
                            if(indexn.indexOf("v14") > 0){
                                var class1 = "", class2 = "";
                                jQuery(".tbl_pregl_lekar_"+n+" tbody").html(" ");
                                if(itemn != null && itemn.length != 0){
                                    var itemn_0;
                                    jQuery.each(itemn[1], function(index_1, itemn_1){
                                        class1 = "broj_v14_"+ n + "_1_"+index_1;
                                        class2 = "broj_v14_"+ n + "_0_"+index_1;
                                        itemn_0 = itemn[0][index_1];
         jQuery(".tbl_pregl_lekar_"+n+" tbody").append(('<tr>'
                 +'<td><input type="text" readonly name="br_pregledi_po_lekari_t'+n+'_1" class="'+class1+' br_priems_po_lekari_t'+n+'_1" value="'+itemn_1+'">'+'</td>'
                 +'<td><input type="text" readonly name="br_pregledi_po_lekari_t'+n+'_0" class="'+class2+' br_priems_po_lekari_t'+n+'_0" value="'+itemn_0+'">'+'</td>' +
                 '</tr>'));
 console.log(index_1, itemn_1, class1, " - ", itemn_0, class2);
                                    });
                                }else{
                                    jQuery(".tbl_pregl_lekar_"+n+" tbody").append('<tr><td>0</td></tr>');
}
 }else
                            if(indexn.indexOf("v15") > 0){}
                            else
                            if(indexn.indexOf("v16") > 0){}
                            else
                            if(indexn.indexOf("v20") > 0){}
                            else
                            if(indexn.indexOf("v21") > 0){}
                            else{
                                jQuery("."+indexn).val(itemn);
                            }
                        });
                    }
                }
                });
        });

        jQuery(".parametri-pregled-pacienti.notacordion").addClass("ui-accordion ui-accordion-icons ui-widget ui-helper-reset")
                .find("h3")
                .addClass("ui-accordion-header ui-helper-reset ui-state-default ui-corner-top ui-corner-bottom")
                .hover(function () {
                    jQuery(this).toggleClass("ui-state-hover");
                })
                .prepend('<span class="ui-icon ui-icon-triangle-1-e"></span>')
                .click(function () {
                    jQuery(this)
                            .toggleClass("ui-accordion-header-active ui-state-active ui-state-default ui-corner-bottom")
                            .find("> .ui-icon").toggleClass("ui-icon-triangle-1-e ui-icon-triangle-1-s").end()
                            .next().toggleClass("ui-accordion-content-active").slideToggle();
                    return false;
                })
                .next()
                .addClass("ui-accordion-content  ui-helper-reset ui-widget-content ui-corner-bottom")
                .hide();
    });
</script>
